package basic1;

public class Person1 {
    //data field
    private String name;
    private String birth;
    private double gpa;
    
    //method : behaviour
    //constructor: Có thể nạp chồng
    public Person1(){
    }
    public void eat(){
        System.out.println("co cai ");
    }

    public Person1(String name, String birth, double gpa) {
        System.out.println("Day la ham khoi tao co tham so");
        this.name = name;
        this.birth = birth;
        this.gpa = gpa;
    }

    public String getName() {
        return name;
    }
    public void setName(String name) {
        this.name = name;
    }
    public String getBirth() {
        return birth;
    }
    public void setBirth(String birth) {
        this.birth = birth;
    }
    public double getGpa() {
        return gpa;
    }
    public void setGpa(double gpa) {
        this.gpa = gpa;
    }
}

